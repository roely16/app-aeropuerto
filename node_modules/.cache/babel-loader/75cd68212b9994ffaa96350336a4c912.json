CAAA,KAAA,cAAA,EAAA;AAEA,QAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,KAAA,EAAA,SAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAAA;AAMA,OARA,MAQA;AAEA;AACA,YAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,8BAAA,CAAA,CAAA;AAEA,YAAA,IAAA,GAAA;AACA,UAAA,UAAA,EAAA,OAAA,CAAA,EADA;AAEA,UAAA,IAAA,EAAA,KAAA,IAAA,CAAA,MAFA;AAGA,UAAA,UAAA,EAAA,KAAA,IAAA,CAAA;AAHA,SAAA;AAMA,aAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,eAAA,GAAA,uBAAA,EAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AAEA,cAAA,QAAA,CAAA,IAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AAEA,YAAA,IAAA,CAAA,IAAA,CAAA;AACA,cAAA,IAAA,EAAA,OADA;AAEA,cAAA,KAAA,EAAA,SAFA;AAGA,cAAA,IAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AAHA,aAAA;AAMA,WARA,MAQA;AAEA,YAAA,IAAA,CAAA,IAAA,CACA,YADA,EAEA,QAAA,CAAA,IAAA,CAAA,OAFA,EAGA,SAHA,EAIA,IAJA,CAIA,YAAA;AAEA,cAAA,YAAA,CAAA,UAAA,CAAA,8BAAA;;AAEA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA;AAAA,eAAA;AAEA,aAVA;AAYA;AAGA,SA5BA;AA8BA;;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AAEA,KAxDA;AAyDA,IAAA,YAzDA,0BAyDA;AAAA;;AAEA,UAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,8BAAA,CAAA,CAAA;AAEA,WAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,eAAA,GAAA,mBAAA,EAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AAEA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AAEA,OALA,EAJA,CAWA;;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,eAAA,GAAA,4BAAA,EAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AAEA,QAAA,MAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AAEA,OANA;AAQA,KA7EA;AA8EA,IAAA,KA9EA,mBA8EA;AAAA;;AAEA,UAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,8BAAA,CAAA,CAAA;AAEA,WAAA,KAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,eAAA,GAAA,YAAA,EAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AAEA,YAAA,QAAA,CAAA,IAAA,EAAA;AAEA,UAAA,YAAA,CAAA,UAAA,CAAA,yBAAA;;AAEA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AAEA;AAEA,OAXA;AAaA;AA/FA,GAlBA;AAmHA,EAAA,QAAA,EAAA;AAEA,IAAA,QAAA,EAAA,oBAAA;AAEA,UAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,8BAAA,CAAA,CAAA;AAEA,aAAA,OAAA,CAAA,MAAA,CAAA,WAAA,KAAA,GAAA,GAAA,OAAA,CAAA,QAAA,CAAA,WAAA,EAAA;AAEA,KARA;AASA,IAAA,OAAA,EAAA,mBAAA;AAEA,aAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AAEA,KAbA;AAcA,IAAA,cAAA,EAAA,0BAAA;AAEA,UAAA,KAAA,IAAA,CAAA,KAAA,IAAA,KAAA,IAAA,CAAA,YAAA,EAAA;AAEA,eAAA,KAAA;AAEA;;AAEA,aAAA,IAAA;AAEA;AAxBA,GAnHA;AA+IA,EAAA,OA/IA,qBA+IA;AAEA,SAAA,YAAA;AAEA;AAnJA,CAAA","sourcesContent":["<template>\n\t<div class=\"home\">\n\t\t<b-navbar sticky style=\"background-color: #0c3757\" toggleable=\"lg\" type=\"dark\">\n\t\t\t<b-navbar-brand href=\"#\">\n\t\t\t\t<b-row align-v=\"center\">\n\t\t\t\t\t<b-col cols=\"2\">\n\t\t\t\t\t\t<font-awesome-icon icon=\"user\" />\n\t\t\t\t\t</b-col>\n\t\t\t\t\t<b-col cols=\"9\">\n\t\t\t\t\t\t<h6 class=\"mb-0\">{{ userName }}</h6>\n\t\t\t\t\t\t<!-- <h6 v-if=\"usuario\" class=\"font-weight-lighter p-0\">{{ usuario.dependencia.toUpperCase() }}</h6> -->\n\t\t\t\t\t\t<h6 class=\"mb-0\" style=\"color: #f2e006\">{{ this.$route.meta.label }}</h6>\n\t\t\t\t\t</b-col>\n\t\t\t\t</b-row>\n\t\t\t</b-navbar-brand>\n\t\t\t<b-navbar-nav class=\"ml-auto\">\n\t\t\t\t<b-nav-item @click=\"show = !show\">\n\t\t\t\t\t<font-awesome-icon size=\"lg\" icon=\"bars\" />\n\t\t\t\t</b-nav-item>\n\t\t\t</b-navbar-nav>\n\t\t</b-navbar>\n\n\t\t<b-sidebar\n\t\t\tid=\"sidebar-backdrop\"\n\t\t\tbackdrop\n\t\t\tshadow\n\t\t\tv-model=\"show\"\n\t\t>\n\t\t\t<div class=\"px-3 py-2\">\n\t\t\t\t<b-row>\n\t\t\t\t\t<b-col>\n\t\t\t\t\t\t<span class=\"font-weight-lighter\">Versión {{ version }}</span>\n\t\t\t\t\t</b-col>\n\t\t\t\t</b-row>\n\t\t\t\t<b-row align-h=\"center\">\n\t\t\t\t\t<b-col cols=\"4\">\n\t\t\t\t\t\t<b-img src=\"./img/user.png\" fluid alt=\"Responsive image\"></b-img>\n\t\t\t\t\t</b-col>\n\t\t\t\t\t<b-col class=\"text-center mt-3\" cols=\"12\">\n\t\t\t\t\t\t<h5>{{ userName }}</h5>\n\t\t\t\t\t\t<!-- <b-link @click=\"modalCuenta = true\">Configuración</b-link> -->\n\t\t\t\t\t</b-col>\n\t\t\t\t</b-row>\n\t\t\t</div>\n\n\t\t\t<nav class=\"mb-3 mt-4\">\n\t\t\t\t<b-nav vertical>\n\t\t\t\t\t<b-nav-item v-for=\"(option, key) in menu\" :key=\"key\" :to=\"{name: option.url, params: {id: option.tipo_reporte}, meta: {label: option.name}}\" >{{ option.nombre }}</b-nav-item>\n\t\t\t\t\t<b-nav-item @click=\"salir\"><span class=\"text-danger\">Salir</span></b-nav-item>\n\t\t\t\t</b-nav>\n\t\t\t</nav>\n\t\t</b-sidebar>\n\n\t\t<b-modal v-model=\"modalCuenta\" title=\"Mi Cuenta\" hide-footer>\n\t\t\t<b-row>\n\t\t\t\t<b-col>\n\t\t\t\t\t<b-card no-body>\n\t\t\t\t\t\t<b-tabs card>\n\t\t\t\t\t\t\t<b-tab title=\"Cambiar Contraseña\" active>\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t<b-form @submit.prevent=\"actualizarClave()\">\n\t\t\t\t\t\t\t\t\t<b-form-group label-class=\"font-weight-bold pt-0\" label=\"Contraseña Actual\">\n\t\t\t\t\t\t\t\t\t\t<b-input-group>\n\t\t\t\t\t\t\t\t\t\t\t<template v-slot:append>\n\t\t\t\t\t\t\t\t\t\t\t\t<b-input-group-text @click=\"pass.show_pass = !pass.show_pass\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t<font-awesome-icon :icon=\"pass.show_pass ? 'eye-slash' : 'eye'\" />\n\t\t\t\t\t\t\t\t\t\t\t\t</b-input-group-text>\n\t\t\t\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t\t\t\t\t<b-form-input :type=\"pass.show_pass ? 'text' : 'password'\" v-model=\"pass.actual\" autocomplete=\"off\" required></b-form-input>\n\t\t\t\t\t\t\t\t\t\t</b-input-group>\n\t\t\t\t\t\t\t\t\t</b-form-group>\n\n\t\t\t\t\t\t\t\t\t<b-form-group label-class=\"font-weight-bold pt-0\" label=\"Nueva Contraseña\">\n\t\t\t\t\t\t\t\t\t\t<b-input-group>\n\t\t\t\t\t\t\t\t\t\t\t<template v-slot:append>\n\t\t\t\t\t\t\t\t\t\t\t\t<b-input-group-text @click=\"pass.show_nuevo_pass = !pass.show_nuevo_pass\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t<font-awesome-icon :icon=\"pass.show_nuevo_pass ? 'eye-slash' : 'eye'\" />\n\t\t\t\t\t\t\t\t\t\t\t\t</b-input-group-text>\n\t\t\t\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t\t\t\t\t<b-form-input autocomplete=\"off\" :type=\"pass.show_nuevo_pass ? 'text' : 'password'\" v-model=\"pass.nueva\" required></b-form-input>\n\t\t\t\t\t\t\t\t\t\t</b-input-group>\n\t\t\t\t\t\t\t\t\t</b-form-group>\n\n\t\t\t\t\t\t\t\t\t<b-form-group label-class=\"font-weight-bold pt-0\" label=\"Repite Nueva Contraseña\">\n\t\t\t\t\t\t\t\t\t\t<b-input-group>\n\t\t\t\t\t\t\t\t\t\t\t<template v-slot:append>\n\t\t\t\t\t\t\t\t\t\t\t\t<b-input-group-text @click=\"pass.show_repite_nuevo_pass = !pass.show_repite_nuevo_pass\">\n\t\t\t\t\t\t\t\t\t\t\t\t\t<font-awesome-icon :icon=\"pass.show_repite_nuevo_pass ? 'eye-slash' : 'eye'\" />\n\t\t\t\t\t\t\t\t\t\t\t\t</b-input-group-text>\n\t\t\t\t\t\t\t\t\t\t\t</template>\n\t\t\t\t\t\t\t\t\t\t\t<b-form-input autocomplete=\"off\" :type=\"pass.show_repite_nuevo_pass ? 'text' : 'password'\" v-model=\"pass.repite_nueva\" required></b-form-input>\n\t\t\t\t\t\t\t\t\t\t</b-input-group>\n\t\t\t\t\t\t\t\t\t</b-form-group>\n\n\t\t\t\t\t\t\t\t\t<b-row class=\"text-center\">\n\t\t\t\t\t\t\t\t\t\t<b-col>\n\t\t\t\t\t\t\t\t\t\t\t<b-button size=\"lg\" type=\"submit\" variant=\"success\">\n\t\t\t\t\t\t\t\t\t\t\t\tActualizar\n\t\t\t\t\t\t\t\t\t\t\t\t<font-awesome-icon icon=\"sync\"></font-awesome-icon>\n\t\t\t\t\t\t\t\t\t\t\t</b-button>\n\t\t\t\t\t\t\t\t\t\t</b-col>\n\t\t\t\t\t\t\t\t\t</b-row>\n\t\t\t\t\t\t\t\t</b-form>\n\n\t\t\t\t\t\t\t</b-tab>\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t</b-tabs>\n\t\t\t\t\t</b-card>\n\t\t\t\t</b-col>\n\t\t\t</b-row>\n\t\t</b-modal>\n\n\t\t<router-view />\n\n\t</div>\n</template>\n\n<script>\n// @ is an alias to /src\n\n\timport Swal from 'sweetalert2'\n\n\texport default {\n\t\tname: 'Home',\n\t\tdata(){\n\t\t\treturn{\n\t\t\t\tmenu: [],\n\t\t\t\tusuario: null,\n\t\t\t\tshow: false,\n\t\t\t\tmodalCuenta: false,\n\t\t\t\tpass: {\n\t\t\t\t\tactual: null,\n\t\t\t\t\tnueva: null,\n\t\t\t\t\trepite_nueva: null,\n\t\t\t\t\tshow_pass: false,\n\t\t\t\t\tshow_nuevo_pass: false,\n\t\t\t\t\tshow_repite_nuevo_pass: false\n\t\t\t\t}\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\tactualizarClave(){\n\n\t\t\t\tif (!this.pass_coinciden) {\n\t\t\t\t\t\n\t\t\t\t\tSwal.fire({\n\t\t\t\t\t\ticon: 'error',\n\t\t\t\t\t\ttitle: 'Oops...',\n\t\t\t\t\t\ttext: 'La nueva contraseña debe coincidir!',\n\t\t\t\t\t})\n\n\t\t\t\t}else{\n\n\t\t\t\t\t// Actualizar clave\n\t\t\t\t\tlet usuario = JSON.parse(localStorage.getItem('usuario-auditoria-transporte'))\n\n\t\t\t\t\tlet data = {\n\t\t\t\t\t\tid_usuario: usuario.id,\n\t\t\t\t\t\tpass: this.pass.actual,\n\t\t\t\t\t\tnuevo_pass: this.pass.nueva\n\t\t\t\t\t}\n\n\t\t\t\t\tthis.axios.post(process.env.VUE_APP_API_URL + '/actualizar_clave.php', data)\n\t\t\t\t\t.then((response) => {\n\n\t\t\t\t\t\tif (response.data.code != 200) {\n\t\t\t\t\t\t\t\n\t\t\t\t\t\t\tSwal.fire({\n\t\t\t\t\t\t\t\ticon: 'error',\n\t\t\t\t\t\t\t\ttitle: 'Oops...',\n\t\t\t\t\t\t\t\ttext: response.data.message,\n\t\t\t\t\t\t\t})\n\n\t\t\t\t\t\t}else{\n\n\t\t\t\t\t\t\tSwal.fire(\n\t\t\t\t\t\t\t\t'Excelente!',\n\t\t\t\t\t\t\t\tresponse.data.message,\n\t\t\t\t\t\t\t\t'success'\n\t\t\t\t\t\t\t).then(() => {\n\n\t\t\t\t\t\t\t\tlocalStorage.removeItem('usuario-auditoria-transporte')\n\n\t\t\t\t\t\t\t\tthis.$router.push({ name: 'login' })\n\n\t\t\t\t\t\t\t})\n\n\t\t\t\t\t\t}\n\t\t\t\t\t\t\n\t\t\t\t\t\t\n\t\t\t\t\t})\n\n\t\t\t\t}\n\n\t\t\t\tconsole.log('actualizar')\n\n\t\t\t},\n\t\t\tobtener_menu(){\n\n\t\t\t\tlet usuario = JSON.parse(localStorage.getItem('usuario-auditoria-transporte'))\n\n\t\t\t\tthis.axios.post(process.env.VUE_APP_API_URL + '/obtener_menu.php', usuario)\n\t\t\t\t.then((response) => {\n\n\t\t\t\t\tthis.menu = response.data\n\t\t\t\t\t\n\t\t\t\t})\n\n\t\t\t\t// Datos Usuario\n\t\t\t\tthis.axios.post(process.env.VUE_APP_API_URL + '/obtener_datos_usuario.php', usuario)\n\t\t\t\t.then((response) => {\n\n\t\t\t\t\tthis.usuario = response.data\n\t\t\t\t\tconsole.log(response.data)\n\t\t\t\t\t\n\t\t\t\t})\n\n\t\t\t},\n\t\t\tsalir(){\n\n\t\t\t\tlet usuario = JSON.parse(localStorage.getItem('usuario-auditoria-transporte'))\n\t\t\t\t\n\t\t\t\tthis.axios.post(process.env.VUE_APP_API_URL + '/salir.php', usuario)\n\t\t\t\t.then((response) => {\n\n\t\t\t\t\tif (response.data) {\n\t\t\t\t\t\t\n\t\t\t\t\t\tlocalStorage.removeItem('usuario-monitoreo-fase2')\n\n\t\t\t\t\t\tthis.$router.push({ name: 'login' })\n\n\t\t\t\t\t}\n\n\t\t\t\t})\n\n\t\t\t}\n\t\t},\n\t\tcomputed: {\n\n\t\t\tuserName: function(){\n\n\t\t\t\tlet usuario = JSON.parse(localStorage.getItem('usuario-auditoria-transporte'))\n\t\t\t\t\n\t\t\t\treturn usuario.nombre.toUpperCase() + ' ' + usuario.apellido.toUpperCase();\n\n\t\t\t},\n\t\t\tversion: function(){\n\n\t\t\t\treturn process.env.VUE_APP_VERSION\n\n\t\t\t},\n\t\t\tpass_coinciden: function(){\n\t\t\t\t\n\t\t\t\tif (this.pass.nueva != this.pass.repite_nueva) {\n\t\t\t\t\t\n\t\t\t\t\treturn false\n\n\t\t\t\t}\n\n\t\t\t\treturn true\n\n\t\t\t}\n\n\t\t\t\t\n\t\t},\n\t\tmounted(){\n\n\t\t\tthis.obtener_menu()\n\n\t\t}\n\t}\n</script>\n"],"sourceRoot":"src/views"}]}